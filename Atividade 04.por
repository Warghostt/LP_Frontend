programa {
  funcao inicio() {
   inteiro num1, num2

   escreva("Digiete o primeiro número: \n")
    leia(num1)

    escreva("Digiete o segundo número: \n")
    leia(num2)
    
    se(num1>num2){
      escreva("Maior número é:  \n", num1)
    }
   senao{
      escreva("Maior número é:  \n", num2)
   }
  }
}
